{
	"name": "wikimedia/fundraiser-unsubscribe",
	"description": "Wikimedia Foundation mailing list update frontend",
	"license": "GPL-2.0-only",
	"support": {
		"irc": "irc://irc.freenode.net/wikimedia-fundraising"
	},
	"require": {
		"php": ">= 7.2.9",
		"coderkungfu/php-queue": "^1.0",
		"predis/predis": "^1.1",
		"twig/twig": "^1.38"
	},
	"require-dev": {
		"jakub-onderka/php-parallel-lint": "1.0.0",
		"jakub-onderka/php-console-highlighter": "0.4.0",
		"mediawiki/minus-x": "0.3.2",
		"mediawiki/mediawiki-codesniffer": "30.0.0"
	},
	"scripts": {
		"fix": [
			"minus-x fix .",
			"phpcbf"
		],
		"test": [
			"parallel-lint . --exclude vendor --exclude node_modules",
			"phpcs -p -s",
			"minus-x check ."
		]
	},
	"extra": {
		"phan-taint-check-plugin": "2.0.1"
	}
}
